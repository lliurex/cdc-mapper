[Unit]
Description=CDC mapper
Wants=network-online.target
After=network-online.target


[Service]
User=root
WorkingDirectory=/usr/share/cdcmapper
ExecStart=/usr/bin/cdc-mapper-launcher
StandardOutput=journal
Restart=always
RestartSec=5
StartLimitBurst=10
StartLimitInterval=200


[Install]
WantedBy=multi-user.target

